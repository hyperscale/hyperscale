{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceRoot}",
                "${workspaceRoot}/bin/hyperscale-cmd",
                "${workspaceRoot}/bazel-hyperscale/external/llvm-project/llvm/include",
                "${workspaceRoot}/bazel-hyperscale/external/llvm-project/clang/include",
                "/usr/local/include",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include/c++/v1"
